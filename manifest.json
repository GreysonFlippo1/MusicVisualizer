{
  "manifest_version": 2,
  "name": "Music Visualizer for Google Playâ„¢ Music",
  "version": "8.5",
  "description": "Music Visualizer for Google Play Music and YouTube. Press ' f2 ' once loaded into a webpage to start visualizing!",
  "web_accessible_resources": ["Bar_Viz.png","Wave_Viz.png","Circle_Viz.png","Ambient_Viz.png"],
  "icons": {                   
    "48": "icon48.png",                   
    "128": "icon128.jpg"
  },
 "browser_action": {
    "default_icon": {                   
      "48": "icon48.png"    
    }
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>" , "https://play.google.com/music/*" , "https://www.youtube.com/*", "https://music.youtube.com/*"],
      "css":["visualizerCSS.css"],
      "js": ["visualizer.js"]
    }
  ],
  "permissions": [
    "storage"
  ]
}